use userrrrr;
drop table cities;
create table citiess
(
id int primary key auto_increment,
city varchar(20) not null
);
drop table userrss;
create table userrss
(
    id      int primary key auto_increment,
    name    varchar(20) not null,
    age     int         not null,
    city_id int         null ,
    foreign key (city_id) references citiess (id)
);
